class Point
   function self(x, y)
      self.x = x
      self.y = y
   end
   function self.__unapply(o)
      return ipairs{ o.x, o.y }
   end
end


p = Point(11, 22)

given p
   case 42 then
      print "forty-two"
   case { x = x, y = y } then
      print("Point: %{x}, %{y}")
end

Point(x, y) = p
print x, y

